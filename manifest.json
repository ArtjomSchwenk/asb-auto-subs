{
  "manifest_version": 3,
  "name": "ASB Auto Subs",
  "description": "Let me cook",
  "version": "1.0",
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "dist/popup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "https://*/"
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_icon": "imgs/takodachi.png"
  }
}